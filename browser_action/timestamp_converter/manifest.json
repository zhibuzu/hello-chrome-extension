{
    "name": "timestamp converter",
    "version": "1.2",
    "description": "timestamp convert to datetime and datetime convert to timestamp",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["storage"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "timestamp converter",
        "default_popup": "popup.html"
    }
}
